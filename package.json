{
  "name": "technoidentity-devfractal",
  "version": "1.2.19",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/technoidentity-devfractal.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "elint:fix": "yarn elint --quiet --fix",
    "elint": "eslint './src/**/*.{js,ts,tsx}'",
    "watch": "tsdx watch --tsconfig tsconfig.build.json",
    "lint:fix": "yarn elint:fix && yarn tlint:fix",
    "lint": "yarn elint && yarn tlint",
    "start": "parcel index.html --open",
    "test:watch": "yarn test --watch",
    "test": "cross-env NODE_ENV='test' jest --passWithNoTests",
    "tlint:fix": "yarn lint --fix",
    "tlint": "yarn tslint -p . -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "types:watch:verbose": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch --preserveWatchOutput",
    "types:watch": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling yarn types --watch",
    "types": "tsc --noEmit -p .",
    "validate:noFix": "yarn lint && yarn types && yarn test",
    "validate": "yarn lint:fix && yarn types && yarn test"
  },
  "dependencies": {
    "devfractal-api": "nandigamchandu/api#1.2.19v",
    "devfractal-crud": "nandigamchandu/crud#1.2.19v",
    "devfractal-forms": "nandigamchandu/forms#1.2.19v",
    "devfractal-router": "nandigamchandu/router#1.2.19v",
    "devfractal-simple": "nandigamchandu/simple#1.2.19v",
    "devfractal-ui": "nandigamchandu/ui#1.2.19v",
    "devfractal-ui-api": "nandigamchandu/ui-api#1.2.19v",
    "devfractal-ui-core": "nandigamchandu/ui-core#1.2.19v",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "cross-env": "6.0.0",
    "eslint": "6.4.0",
    "jest": "24.9.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "react": "16.10.0",
    "react-dom": "16.10.0",
    "react-test-renderer": "16.10.0",
    "rimraf": "3.0.0",
    "tsdx": "0.9.2",
    "tslint": "5.20.0",
    "typescript": "3.6.3",
    "@typescript-eslint/eslint-plugin": "2.7.0",
    "@typescript-eslint/parser": "2.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "lint-staged": "9.4.2",
    "tslint-immutable": "6.0.1",
    "tslint-react": "4.1.0",
    "typescript-tslint-plugin": "0.5.5",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.14.3",
     "eslint-plugin-react-hooks": "2.1.0",
    "tslint-config-prettier": "1.18.0",
    "webpack": "4.41.2",
    "webpack-blocks": "2.0.1",
     "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6"
  },
  "peerDependencies": {
    "bulma": "0.8.0",
    "formik": "1.5.8",
    "technoidentity-utils": "nandigamchandu/utils#1.2.19v",
    "yup": "0.27.0"
  }
